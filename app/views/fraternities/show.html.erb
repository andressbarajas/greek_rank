<p><%= "Highest Rated #{@fraternity.name} Chapters" %></p>

<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <th colspan="1">School Name</th>
    </tr>
  </thead>

  <tbody>
    <% @universities.each do |university| %>
      <tr>
        <td><%= link_to "#{university.name} - #{university.initials}", university %></td>
      </tr>
    <% end %>
  </tbody>
</table>

